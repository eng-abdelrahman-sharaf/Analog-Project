{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run MATLAB project.m",
			"type": "shell",
			"command": "matlab",
			"args": [
				"-nosplash",
				"-nodesktop",
				"-r",
				"run('${workspaceFolder}/project.m'); exit"
			],
			"isBackground": false,
			"problemMatcher": [],
			"group": "build"
		},
		{
			"label": "Run Octave project.m",
			"type": "shell",
			"command": "octave-cli",
			"args": [
				"--quiet",
				"--eval",
				"run('${workspaceFolder}/project.m')"
			],
			"isBackground": false,
			"problemMatcher": [],
			"group": "build"
		},
		{
			"label": "Run Octave project.m (fixed)",
			"type": "shell",
			"command": "octave-cli",
			"args": [
				"--quiet",
				"--eval",
				"run(\"/home/mohammed/Desktop/MatLab analog Project/project.m\")"
			],
			"isBackground": false,
			"problemMatcher": [],
			"group": "build"
		}
	]
}